#!/sbin/busybox sh

# $1 - "default_cpu0_gov"
# $2 - value=
# Created By Dorimanx

# get values from profile
PROFILE=$(cat /data/.dori/.active.profile);
. /data/.dori/"$PROFILE".profile;

if [ "a$2" != "a" ]; then
	default_cpu0_gov=$2;

	echo "$default_cpu0_gov" > /sys/devices/system/cpu/cpufreq/all_cpus/scaling_governor_cpu0;
	/res/uci.sh power_mode $power_mode > /dev/null;
fi;

echo ${default_cpu0_gov};
